{
  "manifest_version": 2,

  "name": "Metova NewTab",
  "description": "Opens a new tab with a custom dashboard",
  "version": "1.0",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
